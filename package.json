{
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "cz": "czg",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "dev": "pnpm -C play dev",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:play": "vue-tsc -p tsconfig.play.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:vite-config": "vue-tsc -p tsconfig.vite-config.json --composite false --noEmit",
    "typecheck:vitest": "vue-tsc -p tsconfig.vitest.json --composite false --noEmit",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "prepare": "husky"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@floating-ui/dom": "^1.0.1",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^10.11.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.13",
    "escape-html": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-svg-loader": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@element-plus/eslint-config": "^0.0.20250630",
    "@eslint/js": "^9.30.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^22.9.0",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.3",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.2",
    "consola": "^2.15.3",
    "csstype": "^2.6.20",
    "cz-git": "^1.11.1",
    "czg": "^1.11.1",
    "eslint": "^9.30.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.11",
    "globals": "^16.2.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.0.11",
    "jsdom": "16.4.0",
    "lint-staged": "^15.5.1",
    "prettier": "^2.7.1",
    "puppeteer": "^22.15.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.10",
    "sass": "^1.89.2",
    "tsx": "^4.19.3",
    "type-fest": "^2.14.0",
    "typescript": "~5.5.4",
    "unplugin-vue-macros": "^2.7.11",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^2.0.5",
    "vue": "^3.5.17",
    "vue-eslint-parser": "9.4.3",
    "vue-router": "^4.0.16",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">= 20"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    },
    "overrides": {
      "typescript": "$typescript",
      "jwa": "^1.4.2",
      "vue-tsc": "^2.2.8",
      "wrench": "npm:wrench-js@^1.5.0"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}